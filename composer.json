{
    "name": "ampliffy/ci-cd",
    "description": "CI/CD library that lets you know which repositories have been affected by a change in any of the repositories it includes",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Ampliffy\\CiCd\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Wilcar Jos√© Angulo S",
            "email": "wilcarjose@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/console": "^6.2",
        "doctrine/collections": "^2.1",
        "doctrine/orm": "^2.14",
        "doctrine/dbal": "^3.5",
        "symfony/cache": "^6.2",
        "vlucas/phpdotenv": "^5.5",
        "doctrine/annotations": "^2.0",
        "symfony/dependency-injection": "^6.2",
        "symfony/config": "^6.2"
    },
    "require-dev": {
        "symfony/test-pack": "^1.0"
    }
}
